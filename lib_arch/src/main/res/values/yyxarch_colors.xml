<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--应用的主要色调,actionBar默认使用该颜色,Toolbar导航栏的底色-->
    <color name="colorPrimary">@color/titleBar</color>
    <!--状态栏颜色,应用的主要暗色调,statusBarColor默认使用该颜色-->
    <color name="colorPrimaryDark">@color/statusBar</color>
    <!--CheckBox,RadioButton,SwitchCompat等一般控件的选中效果默认采用该颜色-->
    <color name="colorAccent">@color/colorTheme</color>
    <!--控件按压时的色调-->
    <color name="colorControlHighlight">@color/pale_grey</color>
    <!--默认按钮的背景颜色-->
    <color name="colorButtonNormal">@color/pale_grey</color>
    <!--窗口背景颜色-->
    <color name="windowBackground">@color/white</color>
    <!--底部栏颜色-->
    <color name="navigationBarColor">@color/white</color>


    <!--主题颜色-->
    <color name="colorTheme">#00C6FF</color>
    <!--所有界面的状态栏的背景颜色-->
    <color name="statusBar">@color/titleBar</color>
    <!--所有界面的标题栏的背景颜色-->
    <color name="titleBar">@color/white</color>
    <!--所有界面的分割线颜色-->
    <color name="item_div_bg">#EAEAEA</color>

</resources>
